  "similarity_rules": {
    "description": "Rules untuk menentukan similarity antar ICD9 procedures",
    "rules": [
      {
        "rule": "same_chapter",
        "similarity_score": 0.8,
        "description": "Prosedur dalam chapter yang sama dianggap mirip"
      },
      {
        "rule": "same_body_system",
        "similarity_score": 0.6,
        "description": "Prosedur pada body system yang sama"
      },
      {
        "rule": "same_first_two_digits",
        "similarity_score": 0.9,
        "description": "Prosedur dengan 2 digit pertama sama (misal: 57.0 vs 57.9)"
      },
      {
        "rule": "same_major_category",
        "similarity_score": 0.5,
        "description": "Prosedur dengan kategori major/minor yang sama"
      }
    ]
  },
